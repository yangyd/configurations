[color]
	ui = true
[core]
	autocrlf = input
	fileMode = false
[push]
	default = simple
[url "git@github.com:"]
	insteadof = git://github.com/
[url "https://github.com/"]

[alias]
	ll = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(green)(%ar)%C(reset) %C(yellow)%an%C(reset)%C(cyan)%d%C(reset)%n''          %C(white)%s%C(reset)' --all
